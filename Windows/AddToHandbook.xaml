<Window x:Class="UZNRKT.Windows.AddToHandbook"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UZNRKT.Windows"
        mc:Ignorable="d"
        Title="AddToHandbook" Height="208" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="140"/>
        </Grid.ColumnDefinitions>
        <StackPanel Margin="4" Grid.Column="0" x:Name="F_StacPanelTextBlock"/>
        <StackPanel Margin="4" Grid.Column="1" x:Name="F_StacPanelFields"/>
        <Button Content="Применить" Height="24" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,4" Click="Button_Click_1"/>
        <Button Content="Отменить" Height="24" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,4" Grid.Column="1" Click="Button_Click"/>
    </Grid>
</Window>

<Window x:Class="UZNRKT.Windows.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UZNRKT.Windows"
        mc:Ignorable="d"
        Title="Вывод на печать" Height="170" Width="200" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <StackPanel Margin="4">
                <TextBlock Text="Акт выполненных работ"/>
                <TextBlock x:Name="F_ApplicationID" Text="Заявка №: *"/>
                <Button Name="F_ButtonApplication" Content="Показать" Margin="2" Click="F_ButtonApplication_Click"/>
            </StackPanel>
            <StackPanel Margin="4">
                <TextBlock Text="Заявки"/>
                <TextBlock x:Name="F_ApplicationCount" Text="Число записей: *"/>
                <Button Name="F_ButtonApplicationCount" Content="Показать" Margin="2" Click="F_ButtonApplicationCount_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
